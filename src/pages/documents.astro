---
import Tabs from '~/components/functional/Tabs';
import Breadcrumb from '~/components/widgets/Breadcrumb.astro';
import Layout from '~/layouts/PageLayout.astro';

const metadata = {
  title: 'Documents',
};
const catalog = '~/assets/images/catalog.png';
---

<Layout metadata={metadata}>
  <Breadcrumb>
    Home
    <svg
      xmlns="http://www.w3.org/2000/svg"
      fill="none"
      viewBox="0 0 24 24"
      stroke-width="1.5"
      stroke="currentColor"
      class="size-4 inline"
    >
      <path stroke-linecap="round" stroke-linejoin="round" d="m8.25 4.5 7.5 7.5-7.5 7.5"></path>
    </svg>
    <span class="text-gray-900">Documents</span>
  </Breadcrumb>

  <div class="w-full px-10 relative" id="popular">
    <div class="xl:w-10/12 mx-auto">
      <div class="mx-auto my-7">
        <h3 class="font-bold text-3xl text-black">Documents</h3>
      </div>

      <Tabs client:load />
    </div>
  </div>
</Layout>
